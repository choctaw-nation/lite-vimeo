{
  "name": "choctaw-nation/lite-vimeo",
  "description": "A web component that loads Vimeo embed iframes faster. Extends Alex Russell (@slightlyoff)'s version to allow unlisted videos.",
  "author": "Choctaw Nation of Oklahoma",
  "repository": {
    "type": "git",
    "url": "git@github.com:choctaw-nation/lite-vimeo.git"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "build": "tsc --project tsconfig.json",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint *.ts --ignore-path .gitignore",
    "lint:prettier": "prettier --check *.ts --ignore-path .gitignore",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lite-vimeo.d.ts",
    "lite-vimeo.js",
    "lite-vimeo.js.map"
  ],
  "license": "MIT",
  "version": "0.1.0",
  "main": "src/index.js",
  "keywords": [
    "web components",
    "vimeo"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-lit": "^1.2.0",
    "parcel": "^2.12.0",
    "prettier": "^2.0.0",
    "typescript": "^3.8.0"
  }
}
