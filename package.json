{
	"name": "lite-vimeo-embed",
	"description": "A web component that loads Vimeo embed iframes faster. Extends Alex Russell (@slightlyoff)'s version to allow unlisted videos.",
	"author": "Choctaw Nation of Oklahoma",
	"type": "module",
	"prettier": "@wordpress/prettier-config",
	"repository": {
		"type": "git",
		"url": "git@github.com:choctaw-nation/lite-vimeo.git"
	},
	"license": "MIT",
	"version": "0.1.0",
	"main": "dist/lite-vimeo-embed.js",
	"targets": {
		"main": {
			"source": "src/index.js",
			"context": "browser",
			"outputFormat": "esmodule",
			"isLibrary": true,
			"sourceMap": false
		}
	},
	"scripts": {
		"start": "parcel demo/index.html --dist-dir build",
		"build": "parcel build"
	},
	"keywords": [
		"web components",
		"vimeo"
	],
	"devDependencies": {
		"@wordpress/prettier-config": "^3.12.0",
		"parcel": "^2.12.0"
	}
}
